  },
  "homepage": "https://github.com/arpitbatra123/lands-upon#readme",
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-img": "^0.10.0",
    "@exifer/exif": "^1.0.0-beta.2",
    "@exifer/gps": "^1.0.0-beta.2",
    "concurrently": "^6.2.0",
    "exifer": "^1.0.0-beta.2",
    "fast-exif": "^1.0.1",
    "feed": "^4.2.2",
    "got": "^11.8.2",
    "sass": "^1.35.1"
  }
